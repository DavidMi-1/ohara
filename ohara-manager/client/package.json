{
  "name": "ohara-manager-client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^8.12.0"
  },
  "scripts": {
    "setup": "yarn && yarn build",
    "start": "react-scripts start",
    "build": "CI=true react-scripts build",
    "lint": "CI=true eslint --max-warnings 0 src cypress",
    "vet": "yarn audit",
    "test": "react-scripts test --env=jsdom",
    "test:watch": "yarn test",
    "test:report": "CI=true yarn test --reporters=default --reporters=jest-junit",
    "test:coverage": "yarn test --coverage --watchAll=false",
    "test:api:open": "cypress open --config integrationFolder=cypress/api",
    "test:api:run": "cypress run --config integrationFolder=cypress/api",
    "test:e2e:open": "cypress open",
    "test:e2e:run": "cypress run"
  },
  "dependencies": {
    "@material-ui/core": "4.2.0",
    "@material-ui/icons": "4.2.0",
    "@material-ui/styles": "4.2.0",
    "@testing-library/cypress": "4.0.4",
    "@testing-library/jest-dom": "4.0.0",
    "@testing-library/react": "8.0.7",
    "classnames": "2.2.6",
    "cypress": "3.4.1",
    "cypress-plugin-retries": "1.2.2",
    "d3v4": "4.2.2",
    "dagre-d3": "0.6.1",
    "deep-diff": "1.0.2",
    "enzyme-adapter-react-16": "1.3.0",
    "eslint-plugin-cypress": "2.0.1",
    "final-form": "4.13.0",
    "jest-junit": "5.1.0",
    "moment": "2.24.0",
    "react": "16.8.4",
    "react-content-loader": "3.1.2",
    "react-document-title": "2.0.3",
    "react-dom": "16.8.4",
    "react-draggable": "3.3.0",
    "react-final-form": "6.0.1",
    "react-modal": "3.8.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "3.0.1",
    "styled-components": "3.3.3",
    "toastr": "2.1.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
